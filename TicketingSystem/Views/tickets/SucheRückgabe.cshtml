@model IEnumerable<TicketingSystem.Models.ticket>

@{
    ViewBag.Title = "SucheRückgabe";
}

<h2>SucheRückgabe</h2>

<p>
    @Html.ActionLink("SucheEingabe", "SucheEingabe")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.customers.username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.staff.username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ticket_categories.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ticket_priorities.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ticket_statuses.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.subject)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ticket_content)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.created_at)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.updated_at)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.completed_at)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.customers.username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.staff.username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ticket_categories.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ticket_priorities.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ticket_statuses.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.subject)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ticket_content)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.created_at)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.updated_at)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.completed_at)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
